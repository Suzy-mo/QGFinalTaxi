<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_heat">
    <fragment
        android:id="@+id/navigation_heat"
        android:name="com.qg.qgtaxiapp.view.fragment.HeatMapFragment"
        android:label="热力图"
        tools:layout="@layout/fragment_heat_map"/>
    <fragment
        android:id="@+id/navigation_flow"
        android:name="com.qg.qgtaxiapp.view.fragment.FlowMapFragment"
        android:label="流向图"
        tools:layout="@layout/fragment_flow_map"/>
    <fragment
        android:id="@+id/navigation_history"
        android:name="com.qg.qgtaxiapp.view.fragment.HistoryMapFragment"
        android:label="历史记录"
        tools:layout="@layout/fragment_history_map"/>
</navigation>